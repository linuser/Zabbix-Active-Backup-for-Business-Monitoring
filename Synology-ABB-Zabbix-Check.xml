                                    </dependencies>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <uuid>b75e92be27664830af0da47a6783dc9a</uuid>
                                    <expression>last(/Synology ABB External SingleScript/abb.sh[status,{#DEVICEID}])=5</expression>
                                    <name>ABB [{#HOSTNAME}]: backup WARNING (Status=5)</name>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                    <dependencies>
                                        <dependency>
                                            <name>ABB: Export script or mount not OK</name>
                                            <expression>last(/Synology ABB External SingleScript/abb.sh[check,{$ABB.EXPORT.MAXAGE},{$ABB.MOUNTPOINT},{$ABB.EXPECT_REMOTE},{$ABB.EXPECT_FSTYPE}])=1</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
