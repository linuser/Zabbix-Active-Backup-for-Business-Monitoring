            <discovery_rules>
                <discovery_rule>
                    <uuid>f185102879ac42409753ae15d436b406</uuid>
                    <name>Active Backup Device Discovery (Single Script)</name>
                    <type>EXTERNAL</type>
                    <key>abb.sh[discovery]</key>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>23cf7ac7ef7d44e5a2f9470d4752c556</uuid>
                            <name>ABB [{#HOSTNAME}] backup bytes</name>
                            <type>EXTERNAL</type>
                            <key>abb.sh[bytes,{#DEVICEID}]</key>
                            <delay>5m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>B</units>
                        </item_prototype>
                        <item_prototype>
                            <uuid>8ca76b17db5e46f38b7c00947e202722</uuid>
                            <name>ABB [{#HOSTNAME}] backup duration</name>
                            <type>EXTERNAL</type>
                            <key>abb.sh[duration,{#DEVICEID}]</key>
                            <delay>5m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>s</units>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <uuid>7493a4b709d546b1bf2a8050a86230d2</uuid>
                                    <expression>last(/Synology ABB External SingleScript/abb.sh[duration,{#DEVICEID}])&gt;={$ABB.BACKUP.MAX.DURATION}</expression>
                                    <name>ABB [{#HOSTNAME}]: backup duration unusually long ({ITEM.LASTVALUE})</name>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                    <dependencies>
                                        <dependency>
                                            <name>ABB: Export script or mount not OK</name>
                                            <expression>last(/Synology ABB External SingleScript/abb.sh[check,{$ABB.EXPORT.MAXAGE},{$ABB.MOUNTPOINT},{$ABB.EXPECT_REMOTE},{$ABB.EXPECT_FSTYPE}])=1</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <uuid>04c8e64b879946e0a1f7a38e63eab618</uuid>
                            <name>ABB [{#HOSTNAME}] age since last success</name>
                            <type>EXTERNAL</type>
                            <key>abb.sh[lastsuccess_age,{#DEVICEID}]</key>
                            <delay>10m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>s</units>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <uuid>6a0137fdce054e1aae87f97aea49a36f</uuid>
                                    <expression>last(/Synology ABB External SingleScript/abb.sh[lastsuccess_age,{#DEVICEID}])&gt;={$ABB.BACKUP.MAX.AGE}</expression>
                                    <name>ABB [{#HOSTNAME}]: last successful backup too old ({ITEM.LASTVALUE})</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                    <dependencies>
                                        <dependency>
                                            <name>ABB: Export script or mount not OK</name>
                                            <expression>last(/Synology ABB External SingleScript/abb.sh[check,{$ABB.EXPORT.MAXAGE},{$ABB.MOUNTPOINT},{$ABB.EXPECT_REMOTE},{$ABB.EXPECT_FSTYPE}])=1</expression>
                                        </dependency>
                                        <dependency>
                                            <name>ABB [{#HOSTNAME}]: backup ERROR (Status=4)</name>
                                            <expression>last(/Synology ABB External SingleScript/abb.sh[status,{#DEVICEID}])=4</expression>
                                        </dependency>
                                    </dependencies>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <uuid>6fa4f0a65c1047eea38940ddc1037123</uuid>
                            <name>ABB [{#HOSTNAME}] repo bytes</name>
                            <type>EXTERNAL</type>
                            <key>abb.sh[repo_bytes,{#DEVICEID}]</key>
                            <delay>30m</delay>
                            <history>30d</history>
                            <trends>180d</trends>
                            <units>B</units>
                        </item_prototype>
                        <item_prototype>
                            <uuid>3014de969874480285d531b821b00b6c</uuid>
                            <name>ABB [{#HOSTNAME}] status</name>
                            <type>EXTERNAL</type>
                            <key>abb.sh[status,{#DEVICEID}]</key>
                            <delay>5m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <valuemap>
